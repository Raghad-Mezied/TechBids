{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "consistent-return": "off",
        "linebreak-style": 0
    },
    "overrides": [
        {
          "files": [ "src/**/*.js" ]
        }
      ]
}
